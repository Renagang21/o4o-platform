{
  "name": "@o4o/signage-pharmacy-extension",
  "version": "1.0.0",
  "description": "Pharmacy Digital Signage Extension - Content selection, playlist management, scheduling, and quick actions for pharmacies",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./manifest": {
      "types": "./dist/manifest.d.ts",
      "import": "./dist/manifest.js"
    },
    "./backend": {
      "types": "./dist/backend/index.d.ts",
      "import": "./dist/backend/index.js"
    },
    "./backend/*": "./dist/backend/*",
    "./frontend": {
      "types": "./dist/frontend/index.d.ts",
      "import": "./dist/frontend/index.js"
    },
    "./frontend/*": "./dist/frontend/*"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@o4o/types": "workspace:*",
    "@o4o/digital-signage-contract": "workspace:*",
    "react": "^18.2.0",
    "express": "^4.21.2",
    "axios": "^1.11.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.2",
    "@types/react": "^18.2.0",
    "typescript": "5.4.5"
  },
  "engines": {
    "node": ">=22.18.0",
    "pnpm": ">=9.0.0"
  }
}
