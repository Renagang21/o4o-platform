{
  "name": "@o4o/cosmetics-partner-extension",
  "version": "1.0.0",
  "description": "Cosmetics Partner Extension - Partner/Influencer affiliate sales management",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./manifest": {
      "types": "./dist/manifest.d.ts",
      "import": "./dist/manifest.js"
    },
    "./backend/*": "./dist/backend/*"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "type-check": "tsc --noEmit",
    "test": "jest --config jest.config.cjs"
  },
  "dependencies": {
    "@o4o/types": "workspace:*",
    "@o4o/shortcodes": "workspace:*",
    "react": "^18.2.0",
    "typeorm": "^0.3.20",
    "express": "^4.21.2"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.2.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "5.4.5"
  },
  "engines": {
    "node": ">=22.18.0",
    "pnpm": ">=9.0.0"
  }
}
