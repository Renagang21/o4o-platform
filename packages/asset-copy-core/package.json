{
  "name": "@o4o/asset-copy-core",
  "version": "1.0.0",
  "description": "Asset Copy Core - Platform-level asset snapshot engine with Resolver pattern",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./entities": {
      "import": "./dist/entities/index.js",
      "types": "./dist/entities/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "npx jest --config jest.config.cjs",
    "test:coverage": "npx jest --config jest.config.cjs --coverage"
  },
  "dependencies": {
    "typeorm": "^0.3.20"
  },
  "peerDependencies": {
    "express": "^4.18.0",
    "reflect-metadata": "^0.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  }
}
