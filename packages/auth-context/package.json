{
  "name": "@o4o/auth-context",
  "version": "1.0.0",
  "description": "Authentication context for O4O Platform",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "./build-minimal.sh || node build.js",
    "build:fallback": "./build-minimal.sh",
    "build:server": "node build-server.js",
    "build:tsc": "npx tsc",
    "type-check": "npx tsc --noEmit"
  },
  "dependencies": {
    "react": "^18.3.1",
    "@o4o/auth-client": "file:../auth-client",
    "@o4o/types": "file:../types",
    "socket.io-client": "^4.7.4"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/socket.io-client": "^1.4.36",
    "typescript": "~5.9.2"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-router-dom": "^7.6.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}