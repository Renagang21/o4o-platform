{
  "name": "@o4o/shortcodes",
  "version": "1.0.0",
  "description": "Shortcode system for O4O Platform",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./dynamic": {
      "types": "./dist/dynamic/index.d.ts",
      "default": "./dist/dynamic/index.js"
    }
  },
  "scripts": {
    "build": "npx tsc --build",
    "type-check": "tsc --noEmit",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "typescript": "5.4.5"
  },
  "peerDependencies": {
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}