{
  "name": "@o4o-apps/cms-core",
  "version": "1.0.0",
  "description": "CMS Core Engine - Templates, CPT, ACF, Views, Menus, Media",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./manifest": {
      "import": "./dist/manifest.js",
      "types": "./dist/manifest.d.ts"
    },
    "./entities": {
      "import": "./dist/entities/index.js",
      "types": "./dist/entities/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@o4o-apps/content-core": "workspace:^",
    "typeorm": "^0.3.20"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
}
