{
  "name": "@o4o/types",
  "version": "1.0.0",
  "description": "Shared TypeScript types for O4O Platform",
  "o4o:packageType": "infra-core",
  "o4o:note": "Infrastructure Core - NOT an AppStore app. Build-time type definitions.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./cpt": {
      "types": "./dist/cpt/index.d.ts",
      "import": "./dist/cpt/index.js"
    },
    "./forum": {
      "types": "./dist/forum.d.ts",
      "import": "./dist/forum.js"
    },
    "./content": {
      "types": "./dist/content.d.ts",
      "import": "./dist/content.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "npx tsc --build",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/react": "^18.2.79",
    "typescript": "5.4.5"
  },
  "peerDependencies": {
    "react": "18.2.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
