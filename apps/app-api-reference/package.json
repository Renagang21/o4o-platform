    "dotenv": "^16.6.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2",
    "tsx": "^4.19.2",
    "tsup": "^8.3.5"
  },
  "engines": {
    "node": ">=22.0.0"
