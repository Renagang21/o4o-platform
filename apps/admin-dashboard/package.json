{
  "name": "@o4o/admin-dashboard",
  "version": "0.5.9",
  "description": "WordPress-style Admin Dashboard for O4O Platform - with ParagraphTestBlock and StandaloneEditor",
  "type": "module",
  "scripts": {
    "dev": "npx vite --port 3001",
    "prebuild": "node scripts/update-version.cjs",
    "build": "vite build",
    "postbuild": "cp public/version.json dist/version.json 2>/dev/null || echo 'version.json already copied'",
    "build:prod": "cross-env NODE_OPTIONS=--max-old-space-size=4096 vite build --mode production",
    "build:with-typecheck": "tsc --noEmit && vite build",
    "preview": "npx vite preview",
    "type-check": "bash -c 'tsc --noEmit'",
    "lint": "eslint --config ../../eslint.config.js src/**/*.{ts,tsx} --report-unused-disable-directives --max-warnings 200 || true",
    "lint:fix": "eslint --config ../../eslint.config.js src/**/*.{ts,tsx} --fix",
    "test": "npx vitest run --passWithNoTests --reporter=verbose || true",
    "test:coverage": "npx vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "build:analyze": "npx vite build --mode production --config vite.config.analyze.ts || echo 'Bundle analysis not configured'"
  },
