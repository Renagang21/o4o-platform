{
  "name": "@o4o/neture-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "tsc && vite build --mode production",
    "prebuild": "node -e \"const fs=require('fs');const pkg=require('./package.json');fs.mkdirSync('public',{recursive:true});fs.writeFileSync('public/version.json',JSON.stringify({version:new Date().toISOString().slice(0,10).replace(/-/g,'.')+'-'+new Date().toTimeString().slice(0,5).replace(':',''),buildDate:new Date().toISOString(),environment:'production'},null,2));\"",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@o4o/auth-client": "workspace:*",
    "@o4o/ui": "workspace:*",
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.6.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.1",
    "@vitejs/plugin-react": "^4.3.0",
    "typescript": "^5.6.0",
    "vite": "^6.0.0",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "@types/node": "^22.0.0"
  }
}
